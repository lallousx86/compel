/*
  advanced variable and object creation test

*/

# declare a counter
var $i 1

# create object
var $obj1.

for $i 10 downto 1
{
  // create dynamic attributes in the object
  var $obj1.$i "i=" $i
}

echo $obj1 "\n"

# pseudo associative array
var $obj2.
var $obj2.1 one
var $obj2.2 two
var $obj2.3 three

for $i 1 to 3
{
  echo $i " is " $obj2.$i "\n"
}